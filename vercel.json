{
    "builds": [
        {
            "src": "chatbot_project/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "pythonVersion": "3.8"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "chatbot_project/wsgi.py"
        }
    ]
}